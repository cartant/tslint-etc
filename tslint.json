{
  "defaultSeverity": "error",
  "extends": [
    "@cartant/tslint-config"
  ],
  "rules": {
    "deprecation": { "severity": "off" },
    "trailing-comma": { "severity": "off" },
    "typedef": {
      "options": [
        "parameter",
        "property-declaration"
      ],
      "severity": "error"
    }
  }
}
